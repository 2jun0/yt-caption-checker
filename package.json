{
  "name": "yt-subtitle-filter",
  "version": "1.3.7",
  "description": "Add a subtitle tag language that you want on the video thumbnail in the YouTube.",
  "scripts": {
    "dev:chrome": "webpack --config=build.config/webpack.dev.js --env browser=chrome",
    "dev:firefox": "webpack --config=build.config/webpack.dev.js --env browser=firefox",
    "build": "npm run build:chrome && npm run build:firefox",
    "build:chrome": "webpack --config=build.config/webpack.prod.js --env browser=chrome",
    "build:firefox": "webpack --config=build.config/webpack.prod.js --env browser=firefox",
    "build:zip": "node ./build.config/zip.js --browser=chrome && node ./build.config/zip.js --browser=firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2jun0/yt-subtitle-filter.git"
  },
  "author": "2jun0",
  "license": "MIT",
  "homepage": "https://github.com/2jun0/yt-subtitle-filter#readme",
  "keywords": [
    "YouTube"
  ],
  "bugs": {
    "url": "https://github.com/2jun0/yt-subtitle-filter/issues"
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^5.8.0"
  }
}
